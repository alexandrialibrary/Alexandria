<div ng-controller="BookListCtrl as booksList">
<table>
	<thead>
		<tr>
			<td>ISBN</td>
			<td>Title</td>
			<td>Author</td>
			<td>Pages (not the one by Apple)</td>
		</tr>
	</thead>
	<tbody>
		<tr ng-repeat="book in booksList.books">
			<td>{{book.isbn}}</td>
			<td>{{book.title}}</td>
			<td>{{book.byline}}</td>
			<td>{{book.pages}}</td>
		</tr>
	</tbody>
</table>
</div>
